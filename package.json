{
  "name": "clean-backend",
  "version": "1.0.0",
  "type": "commonjs",
  "main": "start.js",
  "scripts": {
    "dev": "nodemon src/gateway.js",
    "start": "node start.js",
    "lint": "eslint . --ext .js,.mjs",
    "lint:fix": "eslint . --ext .js,.mjs --fix",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "prepare": "husky"
  },
  "lint-staged": {
    ".js": [
      "eslint --fix",
      "prettier --write"
    ],
    ".svelte": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "http-proxy-middleware": "^3.0.5",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "axios": "^1.13.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.2.7",
    "nodemon": "^3.1.0",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "/tests//*.test.js"
    ]
  }
}