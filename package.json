{
  "name": "clean-backend",
  "version": "1.0.0",
  "type": "module",
  "main": "start.js",
  "scripts": {
    "dev": "nodemon start.js",
    "start": "node start.js",
    "lint": "eslint . --ext .js,.mjs",
    "lint:fix": "eslint . --ext .js,.mjs --fix",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "prepare": "husky"
  },
    "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.svelte": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "dotenv": "^16.4.0",
    "express": "^4.19.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.7",
    "nodemon": "^3.1.0",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4"
  }
}
